<p id="notice"><%= notice %></p>
<h1> <%= link_to 'HPD CITATION LOG', '/pages/citationMenu', class: "invisLink" %></h1>
<h1> <%= link_to 'Code Citations', new_code_citation_path, class: "invisLink" %></h1>
<%= link_to 'New Code Citation', new_code_citation_path, class: "myButton" %>
<br>
<br>
<table>
  <thead>
    <tr>
      <th>Book #</th>
      <th>Issued To?</th>
      <th>Start C#</th>
      <th>End C#</th>
      <th>Issued Date</th>
      <th>Issue By?</th>
      <th>Return Date?</th>
      <th>Received By?</th>
      <th>Can Reissue?</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @code_citations.each do |code_citation| %>
      <tr>
        <td><%= code_citation.book_num %></td>
        <td><%= code_citation.issued_to %></td>
        <td><%= code_citation.start_c_num %></td>
        <td><%= code_citation.end_c_num %></td>
        <td><%= code_citation.issue_date %></td>
        <td><%= code_citation.issue_by %></td>
        <td><%= code_citation.return_date %></td>
        <td><%= code_citation.return_by %></td>
        <td><%= code_citation.empty %></td>
        <td><%= link_to 'Show', code_citation %></td>
        <td><%= link_to 'Edit', edit_code_citation_path(code_citation) %></td>
        <td><%= link_to 'Destroy', code_citation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br><%= link_to 'State', state_citations_path, class: "invisLink"  %> | <%= link_to 'Warning', warning_citations_path, class: "invisLink"  %> | <%= link_to 'Parking', parking_citations_path, class: "invisLink"  %> | <%= link_to 'Civil', civil_citations_path, class: "invisLink"  %> |<%= link_to 'Code', code_citations_path, class: "invisLink"  %>

