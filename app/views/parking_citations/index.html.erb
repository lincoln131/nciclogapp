<p id="notice"><%= notice %></p>
<h1> <%= link_to 'HPD CITATION LOG', '/pages/citationMenu', class: "invisLink" %></h1>
<h1> <%= link_to 'Parking Citations', new_parking_citation_path, class: "invisLink" %></h1>
<%= link_to 'New Parking Citation', new_parking_citation_path, class: "myButton" %>
<table>
  <thead>
    <tr>
      <th>Book num</th>
      <th>Issued to</th>
      <th>Start c num</th>
      <th>End c num</th>
      <th>Issue date</th>
      <th>Issue by</th>
      <th>Return date</th>
      <th>Return by</th>
      <th>Empty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parking_citations.each do |parking_citation| %>
      <tr>
        <td><%= parking_citation.book_num %></td>
        <td><%= parking_citation.issued_to %></td>
        <td><%= parking_citation.start_c_num %></td>
        <td><%= parking_citation.end_c_num %></td>
        <td><%= parking_citation.issue_date %></td>
        <td><%= parking_citation.issue_by %></td>
        <td><%= parking_citation.return_date %></td>
        <td><%= parking_citation.return_by %></td>
        <td><%= parking_citation.empty %></td>
        <td><%= link_to 'Show', parking_citation %></td>
        <td><%= link_to 'Edit', edit_parking_citation_path(parking_citation) %></td>
        <td><%= link_to 'Destroy', parking_citation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br><%= link_to 'State', state_citations_path, class: "invisLink"  %> | <%= link_to 'Warning', warning_citations_path, class: "invisLink"  %> | <%= link_to 'Parking', parking_citations_path, class: "invisLink"  %> | <%= link_to 'Civil', civil_citations_path, class: "invisLink"  %> |<%= link_to 'Code', code_citations_path, class: "invisLink"  %>

