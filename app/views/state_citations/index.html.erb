<p id="notice"><%= notice %></p>

<h1>State Citations</h1>

<table>
  <thead>
    <tr>
      <th>Book num</th>
      <th>Issued to</th>
      <th>Start c num</th>
      <th>End c num</th>
      <th>Issue date</th>
      <th>Issue by</th>
      <th>Return date</th>
      <th>Return by</th>
      <th>Empty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @state_citations.each do |state_citation| %>
      <tr>
        <td><%= state_citation.book_num %></td>
        <td><%= state_citation.issued_to %></td>
        <td><%= state_citation.start_c_num %></td>
        <td><%= state_citation.end_c_num %></td>
        <td><%= state_citation.issue_date %></td>
        <td><%= state_citation.issue_by %></td>
        <td><%= state_citation.return_date %></td>
        <td><%= state_citation.return_by %></td>
        <td><%= state_citation.empty %></td>
        <td><%= link_to 'Show', state_citation %></td>
        <td><%= link_to 'Edit', edit_state_citation_path(state_citation) %></td>
        <td><%= link_to 'Destroy', state_citation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State Citation', new_state_citation_path %>
