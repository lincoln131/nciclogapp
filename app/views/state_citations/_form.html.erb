<%= form_with(model: state_citation, local: true) do |form| %>
  <% if state_citation.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(state_citation.errors.count, "error") %> prohibited this state_citation from being saved:</h2>

      <ul>
      <% state_citation.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  
  <div class="field">
    <%= form.label :book_num %>
    <%= form.text_field :book_num, id: :state_citation_book_num, :required => true, :readonly => state_citation.book_num.present?  %>
  </div>

  <div class="field">
    <%= form.label :issued_to %>
    <%= form.text_field :issued_to, id: :state_citation_issued_to, :required => true, :readonly => state_citation.issued_to.present? %>
  </div>

  <div class="field">
    <%= form.label :start_c_num %>
    <%= form.text_field :start_c_num, id: :state_citation_start_c_num, :required => true, :readonly => state_citation.start_c_num.present? %>
  </div>

  <div class="field">
    <%= form.label :end_c_num %>
    <%= form.text_field :end_c_num, id: :state_citation_end_c_num, :required => true, :readonly => state_citation.end_c_num.present? %>
  </div>

  <div class="field">
    <%= form.label :issue_date %>
    <%= form.text_field :issue_date, id: :state_citation_issue_date, :required => true, :readonly => state_citation.issue_date.present? %>
  </div>

  <div class="field">
    <%= form.label :issue_by %>
    <%= form.text_field :issue_by, id: :state_citation_issue_by, :required => true, :readonly => state_citation.issue_by.present? %>
  </div>

  <div class="field">
    <%= form.label :return_date %>
    <%= form.text_field :return_date, id: :state_citation_return_date, :required => true, :readonly => state_citation.return_date.present? %>
  </div>

  <div class="field">
    <%= form.label :return_by %>
    <%= form.text_area :return_by, id: :state_citation_return_by, :required => true, :readonly => state_citation.return_by.present? %>
  </div>

  <div class="field">
    <%= form.label :empty %>
    <%= form.text_field :empty, id: :state_citation_empty, :required => true, :readonly => state_citation.empty.present? %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
