<p id="notice"><%= notice %></p>
<h1>HPD NCIC LOG</h1>
<h1>Vehicle / Boat Parts</h1>

<table>
  <thead>
    <tr>
      <th>|OCA</th>
      <th>|Operator</th>
      <th>|Officer</th>
      <th>|NCIC#</th>
      <th>|Description</th>
      <th>|Owner Name</th>
      <th>|Owner Phone</th>
      <th>|Date Added</th>
      <th>|Date Removed</th>
      <th>|Removed by?</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicle_boat_parts.each do |vehicle_boat_part| %>
      <tr>
        <td><%= vehicle_boat_part.oca %></td>
        <td><%= vehicle_boat_part.operator_name %></td>
        <td><%= vehicle_boat_part.officer_name %></td>
        <td><%= vehicle_boat_part.ncic_num %></td>
        <td><%= vehicle_boat_part.description %></td>
        <td><%= vehicle_boat_part.owner_name %></td>
        <td><%= vehicle_boat_part.owner_phone %></td>
        <td><%= vehicle_boat_part.date_add %></td>
        <td><%= vehicle_boat_part.date_remove %></td>
        <td><%= vehicle_boat_part.remove_by %></td>
        <td><%= link_to 'Show', vehicle_boat_part %></td>
        <td><%= link_to 'Edit', edit_vehicle_boat_part_path(vehicle_boat_part) %></td>
        <td><%= link_to 'Destroy', vehicle_boat_part, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle Boat Part', new_vehicle_boat_part_path %>
