<p id="notice"><%= notice %></p>

<h1>Code Citaions</h1>

<table>
  <thead>
    <tr>
      <th>Book num</th>
      <th>Issued to</th>
      <th>Start c num</th>
      <th>End c num</th>
      <th>Issue date</th>
      <th>Issue by</th>
      <th>Return date</th>
      <th>Return by</th>
      <th>Empty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @code_citaions.each do |code_citaion| %>
      <tr>
        <td><%= code_citaion.book_num %></td>
        <td><%= code_citaion.issued_to %></td>
        <td><%= code_citaion.start_c_num %></td>
        <td><%= code_citaion.end_c_num %></td>
        <td><%= code_citaion.issue_date %></td>
        <td><%= code_citaion.issue_by %></td>
        <td><%= code_citaion.return_date %></td>
        <td><%= code_citaion.return_by %></td>
        <td><%= code_citaion.empty %></td>
        <td><%= link_to 'Show', code_citaion %></td>
        <td><%= link_to 'Edit', edit_code_citaion_path(code_citaion) %></td>
        <td><%= link_to 'Destroy', code_citaion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Code Citaion', new_code_citaion_path %>
