<p id="notice"><%= notice %></p>

<h1>Parking Citaions</h1>

<table>
  <thead>
    <tr>
      <th>Book num</th>
      <th>Issued to</th>
      <th>Start c num</th>
      <th>End c num</th>
      <th>Issue date</th>
      <th>Issue by</th>
      <th>Return date</th>
      <th>Return by</th>
      <th>Empty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parking_citaions.each do |parking_citaion| %>
      <tr>
        <td><%= parking_citaion.book_num %></td>
        <td><%= parking_citaion.issued_to %></td>
        <td><%= parking_citaion.start_c_num %></td>
        <td><%= parking_citaion.end_c_num %></td>
        <td><%= parking_citaion.issue_date %></td>
        <td><%= parking_citaion.issue_by %></td>
        <td><%= parking_citaion.return_date %></td>
        <td><%= parking_citaion.return_by %></td>
        <td><%= parking_citaion.empty %></td>
        <td><%= link_to 'Show', parking_citaion %></td>
        <td><%= link_to 'Edit', edit_parking_citaion_path(parking_citaion) %></td>
        <td><%= link_to 'Destroy', parking_citaion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

