<p id="notice"><%= notice %></p>

<h1>State Citaions</h1>

<table>
  <thead>
    <tr>
      <th>Book num</th>
      <th>Issued to</th>
      <th>Start c num</th>
      <th>End c num</th>
      <th>Issue date</th>
      <th>Issue by</th>
      <th>Return date</th>
      <th>Return by</th>
      <th>Empty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @state_citaions.each do |state_citaion| %>
      <tr>
        <td><%= state_citaion.book_num %></td>
        <td><%= state_citaion.issued_to %></td>
        <td><%= state_citaion.start_c_num %></td>
        <td><%= state_citaion.end_c_num %></td>
        <td><%= state_citaion.issue_date %></td>
        <td><%= state_citaion.issue_by %></td>
        <td><%= state_citaion.return_date %></td>
        <td><%= state_citaion.return_by %></td>
        <td><%= state_citaion.empty %></td>
        <td><%= link_to 'Show', state_citaion %></td>
        <td><%= link_to 'Edit', edit_state_citaion_path(state_citaion) %></td>
        <td><%= link_to 'Destroy', state_citaion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

